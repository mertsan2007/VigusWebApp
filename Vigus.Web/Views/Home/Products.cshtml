@model IEnumerable<GpusViewModel>

@{
    ViewData["Title"] = "- Products";
}
<h1>Products</h1>
<div class="container-sm">
    @foreach (var gpu in Model)
    {
        <div class="card" style="width: 8rem;">
            <img src="~/Images/defaultgpu.png" class="card-img-top h-100" alt="GPU" width="128" height="128"/>
            <div class="card-body">
                <h5 class="card-title">@gpu.FullGpuName</h5>
            </div>
        </div>
    }
</div>